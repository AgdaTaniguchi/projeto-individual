-- Avaliações
INSERT INTO Avaliacao VALUES (1, 1, '2022-06-12 14:53:01', 'Simplesmente o melhor jogo de todos!!! Não tem muito o que falar, totalmente perfeito. Foi por esse jogo que eu baixei meu primeiro editor de texto, Notepad++, tentando criar servidores para jogar com os meus amigos', 10.00, 10.00, 10.00, 10.00, 10.00);
INSERT INTO Avaliacao VALUES (1, 2, '2022-06-12 13:15:11', 'Tudooooooo revolucionáriooooo', 10.00, 9.00, 10.00, 5.00, 10.00);
INSERT INTO Avaliacao VALUES (1, 4, '2022-06-12 10:58:11', 'Pixels me incomodam.', 8.00, 9.00, 10.00, 9.00, 10.00);
INSERT INTO Avaliacao VALUES (2, 1, '2022-06-12 07:12:23', 'Gosto muito desse jogo, principalmente quando estou em time com os meus amigos!', 9.50, 10.00, 7.00, 9.00, 7.00);
INSERT INTO Avaliacao VALUES (2, 5, '2022-06-12 10:56:43', 'n', 0.00, 0.00, 10.00, 0.00, 0.00);
INSERT INTO Avaliacao VALUES (2, 7, '2022-06-12 11:56:32', 'O jogo é ruim, mas me proporciona momentos incríveis', 10.00, 10.00, 10.00, 10.00, 10.00);
INSERT INTO Avaliacao VALUES (4, 1, '2022-06-12 16:00:01', 'Gosto muito desse jogo, a Glados é tudo de bom, amo demais!!! E a parte multijogador então, nossa, joguei muito com a minha namorada', 10.00, 10.00, 10.00, 10.00, 10.00);
INSERT INTO Avaliacao VALUES (5, 3, '2022-06-12 15:08:35', 'De nerd', 10.00, 4.50, 1.20, 3.40, 0.00);
INSERT INTO Avaliacao VALUES (5, 6, '2022-06-12 12:17:53', 'muito bom, mas entrei em depressão', 9.10, 8.00, 7.00, 8.50, 2.00);
INSERT INTO Avaliacao VALUES (8, 1, '2022-06-12 11:50:26', 'Descobri que gosto muito de jogar pelo Game Pass, fico horas jogando com a minha namorada!!! Só falta o time feminino brasileiro!', 10.00, 10.00, 7.00, 7.00, 10.00);
INSERT INTO Avaliacao VALUES (9, 1, '2022-06-12 06:28:23', 'Gosto muito de jogar com os meus amigos8', 7.00, 8.00, 10.00, 6.00, 10.00);
INSERT INTO Avaliacao VALUES (11, 1, '2022-06-12 08:21:12', 'Não curto muito, mas é legal de jogar com os amigos', 7.50, 5.60, 4.80, 5.50, 6.80);
INSERT INTO Avaliacao VALUES (12, 1, '2022-06-12 10:26:34', 'Gosto muito de jogar, mas não tenho muita paciência pra ele às vezes', 10.00, 10.00, 8.00, 9.00, 8.00);
INSERT INTO Avaliacao VALUES (16, 1, '2022-06-12 11:09:30', 'Amo esse jogo, ele é tudo pra mim', 10.00, 10.00, 10.00, 10.00, 10.00);

